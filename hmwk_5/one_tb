:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7f8cebc0b280 .scope module, "one_tb" "one_tb" 2 3;
 .timescale 0 0;
v0x7f8cebc1d140_0 .var "X", 3 0;
v0x7f8cebc1d1f0_0 .net "Y", 3 0, L_0x7f8cebc1d570;  1 drivers 
v0x7f8cebc1d2a0_0 .var "clk", 0 0;
v0x7f8cebc1d370 .array "expected", 5 0, 3 0;
v0x7f8cebc1d400_0 .var/i "i", 31 0;
v0x7f8cebc1d4d0 .array "mem", 5 0, 3 0;
S_0x7f8cebc049c0 .scope module, "uut" "one" 2 13, 3 1 0, S_0x7f8cebc0b280;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "X"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /OUTPUT 4 "Y"
L_0x7f8cebc1d570 .functor BUFZ 4, v0x7f8cebc1ce10_0, C4<0000>, C4<0000>, C4<0000>;
v0x7f8cebc0ca70_0 .var "A", 3 0;
v0x7f8cebc1cd60_0 .var "B", 3 0;
v0x7f8cebc1ce10_0 .var "C", 3 0;
v0x7f8cebc1ced0_0 .net "X", 3 0, v0x7f8cebc1d140_0;  1 drivers 
v0x7f8cebc1cf80_0 .net "Y", 3 0, L_0x7f8cebc1d570;  alias, 1 drivers
v0x7f8cebc1d070_0 .net "clk", 0 0, v0x7f8cebc1d2a0_0;  1 drivers 
E_0x7f8cebc0d350 .event posedge, v0x7f8cebc1d070_0;
    .scope S_0x7f8cebc049c0;
T_0 ;
    %wait E_0x7f8cebc0d350;
    %load/v 8, v0x7f8cebc1ced0_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x7f8cebc0ca70_0, 0, 8;
    %ix/load 0, 1, 0;
    %load/vp0 8, v0x7f8cebc0ca70_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x7f8cebc1cd60_0, 0, 8;
    %ix/load 0, 1, 0;
    %load/vp0 8, v0x7f8cebc1cd60_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x7f8cebc1ce10_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7f8cebc0b280;
T_1 ;
    %vpi_call 2 15 "$readmemb", "input.txt", v0x7f8cebc1d4d0 {0 0};
    %end;
    .thread T_1;
    .scope S_0x7f8cebc0b280;
T_2 ;
    %vpi_call 2 16 "$readmemb", "expected", v0x7f8cebc1d370 {0 0};
    %end;
    .thread T_2;
    .scope S_0x7f8cebc0b280;
T_3 ;
    %set/v v0x7f8cebc1d2a0_0, 0, 1;
    %set/v v0x7f8cebc1d140_0, 0, 4;
    %set/v v0x7f8cebc1d400_0, 0, 32;
T_3.0 ;
    %load/v 8, v0x7f8cebc1d400_0, 32;
    %cmpi/s 8, 8, 32;
    %jmp/0xz T_3.1, 5;
    %wait E_0x7f8cebc0d350;
    %ix/getv/s 3, v0x7f8cebc1d400_0;
    %load/av 8, v0x7f8cebc1d4d0, 4;
    %set/v v0x7f8cebc1d140_0, 8, 4;
    %wait E_0x7f8cebc0d350;
    %wait E_0x7f8cebc0d350;
    %wait E_0x7f8cebc0d350;
    %ix/getv/s 3, v0x7f8cebc1d400_0;
    %load/av 8, v0x7f8cebc1d370, 4;
    %load/v 12, v0x7f8cebc1d1f0_0, 4;
    %cmp/u 8, 12, 4;
    %inv 4, 1;
    %jmp/0xz  T_3.2, 4;
    %vpi_call 2 32 "$display", "Test failure: X=%d expected Y=%d actual Y=%d", v0x7f8cebc1d140_0, &A<v0x7f8cebc1d370, v0x7f8cebc1d400_0 >, v0x7f8cebc1d1f0_0 {0 0};
    %jmp T_3.3;
T_3.2 ;
    %vpi_call 2 33 "$display", "." {0 0};
T_3.3 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x7f8cebc1d400_0, 32;
    %set/v v0x7f8cebc1d400_0, 8, 32;
    %jmp T_3.0;
T_3.1 ;
    %wait E_0x7f8cebc0d350;
    %vpi_call 2 39 "$finish" {0 0};
    %end;
    .thread T_3;
    .scope S_0x7f8cebc0b280;
T_4 ;
    %delay 1, 0;
    %load/v 8, v0x7f8cebc1d2a0_0, 1;
    %inv 8, 1;
    %set/v v0x7f8cebc1d2a0_0, 8, 1;
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
